{
    "version": 2,
    "buildCommand": "bash build_files.sh",
    "builds": [
        {
            "src": "webapp/ews/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "50mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/webapp/dashboard/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "webapp/ews/wsgi.py"
        }
    ]
}
